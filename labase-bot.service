[Unit]
Description=LaBase Trading Bot
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/labase-trading-alerts
ExecStart=/root/labase-trading-alerts/venv/bin/python /root/labase-trading-alerts/trade_breakout_paper.py
Restart=always
RestartSec=10


[Install]
WantedBy=multi-user.target
